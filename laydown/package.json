{
  "name": "laydown",
  "type": "commonjs",
  "directories": {
    "lib": "./src"
  },  
  "main": "./dist/index.js",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "start": "npx ts-node ./src/index",
    "test": "npx ts-node ./test/test",
    "prepublish": "npm run build",
    "pretest": "npm run build"
  },
  "dependencies": {
    "@jukben/emoji-search": "^2.0.1",
    "commonmark": "file:../../commonmark",
    "emojilib": "^3.0.10",
    "js-md5": "^0.7.3"
  },
  "devDependencies": {
    "@types/js-md5": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "eslint": "^8.35.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
